<% layout('layouts/boilerplate.ejs')%>
<div class="row">
  <% users.forEach(async function(user) { %>
  <div class="card mb-3 p-3">
    <a href="/user/<%= user._id %> "
      ><h5 class="card-title"><%= user.username %></h5></a
    >
    <h5 class="card-subtitle"><%= user.email %></h5>
  </div>

  <% } )%>
</div>
